import wollok.game.*
object arriba {
    method siguiente(position) {
        return position.up(1)
    }
}

object abajo {
    method siguiente(position) {
        return position.down(1)
    }
}

object izquierda {
    method siguiente(position) {
        return position.left(1)
    }
}
object derecha {
    method siguiente(position) {
        return position.right(1)
    }
}